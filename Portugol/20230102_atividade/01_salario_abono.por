programa {

  funcao inicio() {
       
    real salario, abono, novoSalario

    escreva("Salário: ")
    leia(salario)

    escreva("Abono: ")
    leia(abono)

    novoSalario = salario + abono 
    
    escreva("\nNovo Salário: ", novoSalario
    
  }
}

